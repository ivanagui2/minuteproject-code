
<!DOCTYPE digester-rules PUBLIC 
        "-//Jakarta Apache //DTD digester-rules XML V1.0//EN" 
        "http://jakarta.apache.org/commons/digester/dtds/digester-rules.dtd">
<digester-rules>
 <pattern value="database-catalog">
  <object-create-rule classname="net.sf.minuteProject.loader.catalog.databasecatalog.node.DatabaseCatalog"/>
  <set-next-rule methodname="setDatabaseCatalog" paramtype="net.sf.minuteProject.loader.catalog.databasecatalog.node.DatabaseCatalog"/>
  <set-properties-rule/>
  <bean-property-setter-rule  propertyname="name" pattern="name"/>
  <pattern value="databases">
   <object-create-rule classname="net.sf.minuteProject.loader.catalog.databasecatalog.node.Database"/>
   <set-next-rule methodname="setDatabases" paramtype="net.sf.minuteProject.loader.catalog.databasecatalog.node.Database"/>
   <set-properties-rule>
		<alias attr-name="name" prop-name="name" />
		<alias attr-name="version" prop-name="version" />
		<alias attr-name="hibernate-dialect" prop-name="hibernateDialect" />
		<alias attr-name="use-schema" prop-name="useSchema" />
		<alias attr-name="driverClassName" prop-name="driverclassname" />
   </set-properties-rule><!--   
   <bean-property-setter-rule  propertyname="name" pattern="name"/>
   <bean-property-setter-rule  propertyname="version" pattern="version"/>
   <bean-property-setter-rule  propertyname="hibernateDialect" pattern="hibernate-dialect"/>
   <bean-property-setter-rule  propertyname="useSchema" pattern="use-schema"/>
   <bean-property-setter-rule  propertyname="driverclassname" pattern="driverClassName"/>
   --><pattern value="primary-key-policy">
    <object-create-rule classname="net.sf.minuteProject.loader.catalog.databasecatalog.node.PrimaryKeyPolicy"/>
    <set-next-rule methodname="setPrimaryKeyPolicy" paramtype="net.sf.minuteProject.loader.catalog.databasecatalog.node.PrimaryKeyPolicy"/>
    <set-properties-rule/>
    <bean-property-setter-rule  propertyname="type" pattern="type"/>
    <bean-property-setter-rule  propertyname="suffix" pattern="suffix"/>
    <bean-property-setter-rule  propertyname="prefix" pattern="prefix"/>
   </pattern>
  </pattern>
 </pattern>

</digester-rules>