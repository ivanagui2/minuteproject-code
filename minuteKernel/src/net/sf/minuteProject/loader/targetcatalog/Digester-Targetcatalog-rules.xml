
<!DOCTYPE digester-rules PUBLIC 
        "-//Jakarta Apache //DTD digester-rules XML V1.0//EN" 
        "http://jakarta.apache.org/commons/digester/dtds/digester-rules.dtd">
<digester-rules>
 <pattern value="target-technology">
  <object-create-rule classname="net.sf.minuteProject.loader.targetcatalog.node.TargetTechnology"/>
  <set-next-rule methodname="setTargetTechnology" paramtype="net.sf.minuteProject.loader.targetcatalog.node.TargetTechnology"/>
  <set-properties-rule/>
  <bean-property-setter-rule  propertyname="name" pattern="name"/>
  <bean-property-setter-rule  propertyname="version" pattern="version"/>
  <bean-property-setter-rule  propertyname="templateConfigFileName" pattern="template-config-file-name"/>
  <bean-property-setter-rule  propertyname="templateDir" pattern="template-dir"/>
  <bean-property-setter-rule  propertyname="dependsOnTargets" pattern="depends-on-targets"/>
 </pattern>

</digester-rules>