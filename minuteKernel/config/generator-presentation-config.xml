<!DOCTYPE root>
<!-- has to be generated and compared is there is a default - the main config reference a target presentation-config file -->
<presentation>
	<presentation-block type="default">
		<field-blocks>
			<field-block name="field-block-PK" type="PK" visibility="no"/>
			<field-block name="field-block-FK" type="FK" visibility="no" 
				display-entity-block-name="entity-block-parent-reference"
				search-entity-block-name="entity-block-search-SUC" />
		</field-blocks>
		<entity-blocks>
			<entity-block name="entity-block-main" strategy="VMtemplate"></entity-block>
			<entity-block name="entity-block-child" strategy="VMtemplate"></entity-block>
			<entity-block name="entity-block-parent" strategy="VMtemplate"></entity-block>
			<entity-block name="entity-block-parent-reference" strategy="VMtemplateparentRef"></entity-block>
			<entity-block name="entity-block-search" strategy="VMtemplateparentRef"></entity-block>
		</entity-blocks>
		<flow-blocks>
			<flow-block name="flow-block-search-entity" strategy="VMtemplate">
				<block refname="entity-block-search"/>
			</flow-block>
			<flow-block name="flow-block-display-entity" strategy="VMtemplateMainParentChildrenWith_ParentRefForPseudoMasterData_NoReturnRef">
				<block refname="entity-block-main"/>
				<block refname="entity-block-parent" strategy="allAsTheyCome"/>
				<block refname="entity-block-children" strategy="allAsTheyCome"/>
			</flow-block>			
		</flow-blocks>
		<assembly-blocks>
			<assembly-block name=""/>
		</assembly-blocks>
	</presentation-block>
	<presentation-block type="custom">
		<field-blocks>
			<field-block name="field-block-PK" type="PK" visibility="no"/>
			<field-block name="field-block-FK" type="FK" visibility="no" 
				display-entity-block-name="entity-block-parent-reference"
				search-entity-block-name="entity-block-search-SUC" />
		</field-blocks>
		<entity-blocks>
			<entity-block name="entity-block-main" strategy="VMtemplate"></entity-block>
			<entity-block name="entity-block-child" strategy="VMtemplate"></entity-block>
			<entity-block name="entity-block-parent" strategy="VMtemplate"></entity-block>
			<entity-block entity="DETTE" type="entity-block-parent-reference" 
				name="dette-block-parent-reference"
				strategy="VMtemplateParentRef">
				<field name="ORIGINE"/>
				<field name="DOCUMENT"/>
			</entity-block>
			<entity-block name="entity-block-search" strategy="VMtemplateparentRef"></entity-block>
		</entity-blocks>
		<flow-blocks>
			<flow-block name="flow-block-search-entity" strategy="VMtemplate">
				<block refname="entity-block-search"/>
			</flow-block>
			<flow-block name="flow-block-display-entity" strategy="VMtemplateMainParentChildrenWith_ParentRefForPseudoMasterData_NoReturnRef">
				<block refname="entity-block-main"/>
				<block refname="entity-block-parent" strategy="allAsTheyCome"/>
				<block refname="entity-block-children" strategy="allAsTheyCome"/>
			</flow-block>			
		</flow-blocks>
		<assembly-blocks>
			<assembly-block name=""/>
		</assembly-blocks>
	</presentation-block>	
	<entity-blocks>
		<model-entity-blocks default="normalOtherwiseSpecified"> <!-- take the default for all the entities -->
			<entity name="DETTES">
				 <default-model-blocks>
					 <display>
						 <!-- every block returns an order list -->
						 <block-entity-main name="" strategy="">
							 <!-- strategy ex : no PK/FK safe towards MasterData, Pseudo via linkParent -->
							 <!-- gives an order list (of couples values (field, value, icone, comments, i18n, security wrapper...)) to the strategy method-->
							 
						 </block-entity-main>
						 <block-entity-child name="">
							 
						 </block-entity-child>
						 <block-entity-parent name="">
							 
						 </block-entity-parent>
						 <block-entity-linkParent name="">
							 <fields>
								 <field>
									 origine
								 </field>
								 <field>
									 document
								 </field>								 
							 </fields>
						 </block-entity-linkParent>	
					 </display><!---->
					 <search>
						 <block-entity-search>
							 
						 </block-entity-search>
					 </search>
				 </default-model-blocks>
			</entity>
		</model-entity-blocks>
		<custom-blocks>
			
		</custom-blocks>
	</entity-blocks>
	<navigation-blocks>
		<model-navigation-blocks>
			<entity name="">
				<actions>
					<action name="add" function="add">
						
					</action>
					<action name="search" function="search">
						
					</action>	
					<action name="edit" function="edit">
						
					</action>
					<action name="addChild" function="addChild">
						
					</action>										
				</actions>
			</entity>			
		</model-navigation-blocks>
	</navigation-blocks>
	<assembly-blocks>
		<!-- business case partern : management business pattern-->
	</assembly-blocks>
</presentation>