<target name="Propel ORM">		
	<architecture-target name="propel"></architecture-target>	
	<plugin name="propelUtils" className="net.sf.minuteProject.plugin.propel.PropelUtils"></plugin>
	
  	<template-target 
    	name="Propel config"
		outputdir="propel">
			 
		<template name="Propel.orm.xml" templateFileName="Propel.orm.xml.vm" 
			add-model-name="false" package-root=""
			add-model-dir-name="false"		
			subdir="" outputsubdir="" technicalPackage=""
			file-extension="xml" file-prefix="schema" 
			model-specific="true">
		</template>
		
		<template name="Propel.build.properties" templateFileName="Propel.build.properties.vm" 
			add-model-name="false" package-root=""
			add-model-dir-name="false"		
			subdir="" outputsubdir="" technicalPackage=""
			file-extension="properties" file-prefix="build" 
			model-specific="true">
		</template>
		
		<template name="Propel.runtime.conf.xml" templateFileName="Propel.runtime.conf.xml.vm" 
			add-model-name="false" package-root=""
			add-model-dir-name="false"		
			subdir="" outputsubdir="" technicalPackage=""
			file-extension="xml" file-prefix="runtime-conf" 
			model-specific="true">
		</template>

										
	</template-target>	

</target>