<?xml version="1.0"?>

<!DOCTYPE digester-rules PUBLIC 
        "-//Jakarta Apache //DTD digester-rules XML V1.0//EN" 
        "http://jakarta.apache.org/commons/digester/dtds/digester-rules.dtd">
<digester-rules>
  <pattern value="generator-config/configuration">		
  <set-properties-rule>
	<alias attr-name="name" prop-name="name" />
  </set-properties-rule>
  <pattern value="model">
	<object-create-rule classname="net.sf.minuteProject.configuration.bean.Model"/>	  
	<set-next-rule methodname="setModel" paramtype="net.sf.minuteProject.configuration.bean.Model"/>
	<set-properties-rule>
		<alias attr-name="name" prop-name="name" />
		<alias attr-name="version" prop-name="version" />
		<alias attr-name="package-root" prop-name="packageRoot" />
	</set-properties-rule>
	<set-properties-rule/>		  
	<pattern value="data-model">
		<object-create-rule classname="net.sf.minuteProject.configuration.bean.DataModel"/>		
		<set-next-rule methodname="setDataModel" paramtype="net.sf.minuteProject.configuration.bean.DataModel"/>		
		<bean-property-setter-rule pattern="schema"/>	
		<pattern value="fileSource">
			<object-create-rule classname="net.sf.minuteProject.configuration.bean.FileSource"/>	
			<set-next-rule methodname="setFileSource" paramtype="net.sf.minuteProject.configuration.bean.FileSource"/>			
			<set-properties-rule/>		  
			<bean-property-setter-rule pattern="name"/>			
			<bean-property-setter-rule pattern="dir"/>		
		</pattern>	
		<pattern value="driver">
			<object-create-rule classname="net.sf.minuteProject.configuration.bean.connection.Driver"/>	
			<set-next-rule methodname="setDriver" paramtype="net.sf.minuteProject.configuration.bean.connection.Driver"/>			
			<set-properties-rule/>		  
			<bean-property-setter-rule pattern="name"/>			
			<bean-property-setter-rule pattern="groupId"/>		
			<bean-property-setter-rule pattern="artifactId"/>		
			<bean-property-setter-rule pattern="version"/>				
		</pattern>							
		<pattern value="dataSource">
			<object-create-rule classname="org.apache.commons.dbcp.BasicDataSource"/>	
			<set-next-rule methodname="setBasicDataSource" paramtype="org.apache.commons.dbcp.BasicDataSource"/>			
			<set-properties-rule/>		  
			<bean-property-setter-rule pattern="driverClassName"/>			
			<bean-property-setter-rule pattern="url"/>		
			<bean-property-setter-rule pattern="username"/>		
			<bean-property-setter-rule pattern="password"/>				
		</pattern>			
		<pattern value="primaryKeyPolicy">
			<object-create-rule classname="net.sf.minuteProject.configuration.bean.strategy.datamodel.PrimaryKeyPolicy"/>	
			<set-next-rule methodname="setPrimaryKeyPolicy" paramtype="net.sf.minuteProject.configuration.bean.strategy.datamodel.PrimaryKeyPolicy"/>			
			<set-properties-rule/>		  
			<bean-property-setter-rule pattern="oneGlobal"/>			
			<bean-property-setter-rule pattern="oneForEachTable"/>		
			<pattern value="primaryKeyPolicyPattern">
				<object-create-rule classname="net.sf.minuteProject.configuration.bean.strategy.datamodel.PrimaryKeyPolicyPattern"/>		
				<set-next-rule methodname="addPrimaryKeyPolicyPattern" paramtype="net.sf.minuteProject.configuration.bean.strategy.datamodel.PrimaryKeyPolicyPattern"/>			  				
				<set-properties-rule/>		  
				<bean-property-setter-rule pattern="name"/>		
				<bean-property-setter-rule pattern="prefix"/>	
				<bean-property-setter-rule pattern="suffix"/>	
				<bean-property-setter-rule pattern="sequenceName"/>		
			</pattern>					
		</pattern>
	</pattern>
	<pattern value="function-model">
		<object-create-rule classname="net.sf.minuteProject.configuration.bean.FunctionModel"/>		
		<set-next-rule methodname="setFunctionModel" paramtype="net.sf.minuteProject.configuration.bean.FunctionModel"/>	
		<pattern value="catalog-generation-condition">
			<object-create-rule classname="net.sf.minuteProject.configuration.bean.condition.CatalogGenerationCondition"/>		
			<set-next-rule methodname="setCatalogGenerationCondition" paramtype="net.sf.minuteProject.configuration.bean.condition.CatalogGenerationCondition"/>	
			<pattern value="generation-condition">
				<object-create-rule classname="net.sf.minuteProject.configuration.bean.GenerationCondition"/>		
				<set-next-rule methodname="setGenerationCondition" paramtype="net.sf.minuteProject.configuration.bean.GenerationCondition"/>	
				<set-properties-rule>
					<alias attr-name="default-type" prop-name="defaultType" />
					<alias attr-name="exclude-tables" prop-name="excludeTables" />
					<alias attr-name="exclude-views" prop-name="excludeViews" />
				</set-properties-rule>	
				<pattern value="condition">
					<object-create-rule classname="net.sf.minuteProject.configuration.bean.Condition"/>		
					<set-next-rule methodname="addCondition" paramtype="net.sf.minuteProject.configuration.bean.Condition"/>		
					<set-properties-rule>
						<alias attr-name="type" prop-name="type" />
						<alias attr-name="startsWith" prop-name="startsWith" />
						<alias attr-name="result" prop-name="result" />		
					</set-properties-rule>							
				</pattern>					
			</pattern>	
		</pattern>
		<pattern value="function-generation-condition">
			<object-create-rule classname="net.sf.minuteProject.configuration.bean.condition.FunctionGenerationCondition"/>		
			<set-next-rule methodname="setFunctionGenerationCondition" paramtype="net.sf.minuteProject.configuration.bean.condition.FunctionGenerationCondition"/>	
			<pattern value="generation-condition">
				<object-create-rule classname="net.sf.minuteProject.configuration.bean.GenerationCondition"/>		
				<set-next-rule methodname="setGenerationCondition" paramtype="net.sf.minuteProject.configuration.bean.GenerationCondition"/>	
				<set-properties-rule>
					<alias attr-name="default-type" prop-name="defaultType" />
				</set-properties-rule>	
				<pattern value="condition">
					<object-create-rule classname="net.sf.minuteProject.configuration.bean.Condition"/>		
					<set-next-rule methodname="addCondition" paramtype="net.sf.minuteProject.configuration.bean.Condition"/>		
					<set-properties-rule>
						<alias attr-name="type" prop-name="type" />
						<alias attr-name="startsWith" prop-name="startsWith" />
						<alias attr-name="result" prop-name="result" />		
					</set-properties-rule>							
				</pattern>					
			</pattern>	
		</pattern>
		<pattern value="business-package">
			<object-create-rule classname="net.sf.minuteProject.configuration.bean.BusinessPackage"/>		
			<set-next-rule methodname="setBusinessPackage" paramtype="net.sf.minuteProject.configuration.bean.BusinessPackage"/>	
			<set-properties-rule>
				<alias attr-name="default" prop-name="defaultPackage" />
				<alias attr-name="default-type" prop-name="defaultPackageType" />	
				<alias attr-name="auto-package-type" prop-name="autoPackageType" />	
			</set-properties-rule>					
			<pattern value="condition">
				<object-create-rule classname="net.sf.minuteProject.configuration.bean.Condition"/>		
				<set-next-rule methodname="addCondition" paramtype="net.sf.minuteProject.configuration.bean.Condition"/>		
				<set-properties-rule>
					<alias attr-name="type" prop-name="type" />
					<alias attr-name="startsWith" prop-name="startsWith" />
					<alias attr-name="result" prop-name="result" />	
				</set-properties-rule>							
			</pattern>					
		</pattern>
	</pattern>	
	<pattern value="business-model">
		<object-create-rule classname="net.sf.minuteProject.configuration.bean.BusinessModel"/>		
		<set-next-rule methodname="setBusinessModel" paramtype="net.sf.minuteProject.configuration.bean.BusinessModel"/>	
		<pattern value="generation-condition">
			<object-create-rule classname="net.sf.minuteProject.configuration.bean.GenerationCondition"/>		
			<set-next-rule methodname="setGenerationCondition" paramtype="net.sf.minuteProject.configuration.bean.GenerationCondition"/>	
			<set-properties-rule>
				<alias attr-name="default-type" prop-name="defaultType" />
			</set-properties-rule>	
			<pattern value="condition">
				<object-create-rule classname="net.sf.minuteProject.configuration.bean.Condition"/>		
				<set-next-rule methodname="addCondition" paramtype="net.sf.minuteProject.configuration.bean.Condition"/>		
				<set-properties-rule>
					<alias attr-name="type" prop-name="type" />
					<alias attr-name="startsWith" prop-name="startsWith" />
					<alias attr-name="result" prop-name="result" />		
				</set-properties-rule>							
			</pattern>					
		</pattern>	
		<pattern value="business-package">
			<object-create-rule classname="net.sf.minuteProject.configuration.bean.BusinessPackage"/>		
			<set-next-rule methodname="setBusinessPackage" paramtype="net.sf.minuteProject.configuration.bean.BusinessPackage"/>	
			<set-properties-rule>
				<alias attr-name="default" prop-name="defaultPackage" />
			</set-properties-rule>					
			<pattern value="condition">
				<object-create-rule classname="net.sf.minuteProject.configuration.bean.Condition"/>		
				<set-next-rule methodname="addCondition" paramtype="net.sf.minuteProject.configuration.bean.Condition"/>		
				<set-properties-rule>
					<alias attr-name="type" prop-name="type" />
					<alias attr-name="startsWith" prop-name="startsWith" />
					<alias attr-name="result" prop-name="result" />		
				</set-properties-rule>							
			</pattern>					
		</pattern>
		<pattern value="xml-enrichment">
			<object-create-rule classname="net.sf.minuteProject.configuration.bean.enrichment.XmlEnrichment"/>		
			<set-next-rule methodname="setXmlEnrichment" paramtype="net.sf.minuteProject.configuration.bean.enrichment.XmlEnrichment"/>					
			<set-properties-rule>
				<alias attr-name="name" prop-name="name" />	
				<alias attr-name="baseElement" prop-name="baseElement" />		
			</set-properties-rule>
		</pattern>
		<pattern value="enrichment">
			<object-create-rule classname="net.sf.minuteProject.configuration.bean.enrichment.Enrichment"/>		
			<set-next-rule methodname="setEnrichment" paramtype="net.sf.minuteProject.configuration.bean.enrichment.Enrichment"/>					
			<pattern value="conventions">
				<object-create-rule classname="net.sf.minuteProject.configuration.bean.enrichment.convention.Conventions"/>		
				<set-next-rule methodname="setConventions" paramtype="net.sf.minuteProject.configuration.bean.enrichment.convention.Conventions"/>		
				<set-properties-rule>
					<alias attr-name="name" prop-name="name" />	
					<alias attr-name="alias" prop-name="alias" />
				</set-properties-rule>	
				<pattern value="view-primary-key-convention">
					<object-create-rule classname="net.sf.minuteProject.configuration.bean.enrichment.convention.ViewPrimaryKeyConvention"/>		
					<set-next-rule methodname="addConvention" paramtype="net.sf.minuteProject.configuration.bean.enrichment.convention.Convention"/>		
					<set-properties-rule>
						<alias attr-name="type" prop-name="type" />	
						<alias attr-name="default-primary-key-names" prop-name="defaultPrimaryKeyNames" />
					</set-properties-rule>	
	            </pattern>		
				<pattern value="table-default-primary-key-convention">
					<object-create-rule classname="net.sf.minuteProject.configuration.bean.enrichment.convention.TableDefaultPrimaryKeyConvention"/>		
					<set-next-rule methodname="addConvention" paramtype="net.sf.minuteProject.configuration.bean.enrichment.convention.Convention"/>		
					<set-properties-rule>
						<alias attr-name="type" prop-name="type" />	
						<alias attr-name="default-primary-key-names" prop-name="defaultPrimaryKeyNames" />
					</set-properties-rule>	
	            </pattern>		            		
				<pattern value="view-foreign-key-convention">
					<object-create-rule classname="net.sf.minuteProject.configuration.bean.enrichment.convention.ViewForeignKeyConvention"/>		
					<set-next-rule methodname="addConvention" paramtype="net.sf.minuteProject.configuration.bean.enrichment.convention.ViewForeignKeyConvention"/>		
					<set-properties-rule>
						<alias attr-name="type" prop-name="type" />	
						<alias attr-name="default-primary-key-names" prop-name="defaultPrimaryKeyNames" />
					</set-properties-rule>	
	            </pattern>
				<pattern value="version-convention">
					<object-create-rule classname="net.sf.minuteProject.configuration.bean.enrichment.convention.VersionConvention"/>		
					<set-next-rule methodname="addConvention" paramtype="net.sf.minuteProject.configuration.bean.enrichment.convention.VersionConvention"/>		
					<set-properties-rule>
						<alias attr-name="type" prop-name="type" />	
						<alias attr-name="default-value" prop-name="defaultValue" />
					</set-properties-rule>	
	            </pattern>	  
				<pattern value="column-naming-convention">
					<object-create-rule classname="net.sf.minuteProject.configuration.bean.enrichment.convention.ColumnNamingConvention"/>		
					<set-next-rule methodname="addConvention" paramtype="net.sf.minuteProject.configuration.bean.enrichment.convention.ColumnNamingConvention"/>		
					<set-properties-rule>
						<alias attr-name="type" prop-name="type" />	
						<alias attr-name="pattern-to-strip" prop-name="patternToStrip" />
					</set-properties-rule>	
	            </pattern>	
				<pattern value="reference-naming-convention">
					<object-create-rule classname="net.sf.minuteProject.configuration.bean.enrichment.convention.ReferenceNamingConvention"/>		
					<set-next-rule methodname="addConvention" paramtype="net.sf.minuteProject.configuration.bean.enrichment.convention.ReferenceNamingConvention"/>		
					<set-properties-rule>
						<alias attr-name="type" prop-name="type" />	
						<alias attr-name="is-to-plurialize" prop-name="isToPlurialize" />	
						<alias attr-name="default-value" prop-name="defaultValue" />
					</set-properties-rule>	
	            </pattern>		            
				<pattern value="entity-naming-convention">
					<object-create-rule classname="net.sf.minuteProject.configuration.bean.enrichment.convention.EntityNamingConvention"/>		
					<set-next-rule methodname="addConvention" paramtype="net.sf.minuteProject.configuration.bean.enrichment.convention.EntityNamingConvention"/>		
					<set-properties-rule>
						<alias attr-name="type" prop-name="type" />	
						<alias attr-name="pattern-to-strip" prop-name="patternToStrip" />
					</set-properties-rule>	
	            </pattern>	            	                      
            </pattern>
			<pattern value="package">
				<object-create-rule classname="net.sf.minuteProject.configuration.bean.enrichment.Package"/>		
				<set-next-rule methodname="addPackage" paramtype="net.sf.minuteProject.configuration.bean.enrichment.Package"/>		
				<set-properties-rule>
					<alias attr-name="name" prop-name="name" />	
					<alias attr-name="alias" prop-name="alias" />
				</set-properties-rule>	
				<pattern value="security-color">
					<object-create-rule classname="net.sf.minuteProject.configuration.bean.enrichment.security.SecurityColor"/>		
					<set-next-rule methodname="setSecurityColor" paramtype="net.sf.minuteProject.configuration.bean.enrichment.security.SecurityColor"/>		
					<set-properties-rule>
						<alias attr-name="name" prop-name="name" />	
						<alias attr-name="roles" prop-name="roles" />
					</set-properties-rule>	
	            </pattern>	
	            <pattern value="entity-group">
					<object-create-rule classname="net.sf.minuteProject.configuration.bean.enrichment.group.EntityGroup"/>		
					<set-next-rule methodname="addEntityGroup" paramtype="net.sf.minuteProject.configuration.bean.enrichment.group.EntityGroup"/>		
					<set-properties-rule>
						<alias attr-name="name" prop-name="name" />	
						<alias attr-name="entities" prop-name="entities" />	
						<alias attr-name="is-visible" prop-name="isVisible" />
					</set-properties-rule>	
	            </pattern>	
            </pattern>
			<pattern value="entity">
				<object-create-rule classname="net.sf.minuteProject.configuration.bean.enrichment.Entity"/>		
				<set-next-rule methodname="addEntity" paramtype="net.sf.minuteProject.configuration.bean.enrichment.Entity"/>		
				<set-properties-rule>
					<alias attr-name="name" prop-name="name" />	
					<alias attr-name="alias" prop-name="alias" />
					<alias attr-name="type" prop-name="type" />
					<alias attr-name="structure" prop-name="structure" />		
					<alias attr-name="content-type" prop-name="contentType" />	
					<alias attr-name="is-searchable" prop-name="searchable" />
					<alias attr-name="is-link-entity" prop-name="linkEntity" />
					<alias attr-name="is-transfer-entity" prop-name="transferEntity" />
					<alias attr-name="comment" prop-name="comment" />
					<alias attr-name="description" prop-name="description" />
				</set-properties-rule>	
				<pattern value="action">
					<object-create-rule classname="net.sf.minuteProject.configuration.bean.enrichment.Action"/>		
					<set-next-rule methodname="addAction" paramtype="net.sf.minuteProject.configuration.bean.enrichment.Action"/>		
					<set-properties-rule>
						<alias attr-name="name" prop-name="name" />	
					</set-properties-rule>	
	            </pattern>				
				<pattern value="field-group">
					<object-create-rule classname="net.sf.minuteProject.configuration.bean.enrichment.group.FieldGroup"/>		
					<set-next-rule methodname="addFieldGroup" paramtype="net.sf.minuteProject.configuration.bean.enrichment.group.FieldGroup"/>		
					<set-properties-rule>
						<alias attr-name="name" prop-name="name" />	
						<alias attr-name="fields" prop-name="fields" />	
						<alias attr-name="is-visible" prop-name="isVisible" />
					</set-properties-rule>	
	            </pattern>				
				<pattern value="security-color">
					<object-create-rule classname="net.sf.minuteProject.configuration.bean.enrichment.security.SecurityColor"/>		
					<set-next-rule methodname="setSecurityColor" paramtype="net.sf.minuteProject.configuration.bean.enrichment.security.SecurityColor"/>		
					<set-properties-rule>
						<alias attr-name="name" prop-name="name" />	
						<alias attr-name="right" prop-name="right" />
					</set-properties-rule>	
	            </pattern>	
				<pattern value="entity-secured-access">
					<object-create-rule classname="net.sf.minuteProject.configuration.bean.enrichment.security.EntitySecuredAccess"/>		
					<set-next-rule methodname="setEntitySecuredAccess" paramtype="net.sf.minuteProject.configuration.bean.enrichment.security.EntitySecuredAccess"/>		
					<set-properties-rule>
						<alias attr-name="name" prop-name="name" />	
						<alias attr-name="roles" prop-name="roles" />
						<alias attr-name="profiles" prop-name="profiles" />		
						<alias attr-name="filters" prop-name="filters" />	
					</set-properties-rule>	
				</pattern>				
				<pattern value="semantic-reference">
					<object-create-rule classname="net.sf.minuteProject.configuration.bean.enrichment.SemanticReference"/>		
					<set-next-rule methodname="setSemanticReference" paramtype="net.sf.minuteProject.configuration.bean.enrichment.SemanticReference"/>	
					<set-properties-rule>
						<alias attr-name="name" prop-name="name" />
					</set-properties-rule>
					<pattern value="sql-path">
						<object-create-rule classname="net.sf.minuteProject.configuration.bean.enrichment.path.SqlPath"/>		
						<set-next-rule methodname="addSqlPath" paramtype="net.sf.minuteProject.configuration.bean.enrichment.path.SqlPath"/>	
						<set-properties-rule>
							<alias attr-name="path" prop-name="path" />
							<alias attr-name="root" prop-name="root" />
							<alias attr-name="name" prop-name="name" />
						</set-properties-rule>
					</pattern>						
				</pattern>					
				<pattern value="property">
					<object-create-rule classname="net.sf.minuteProject.configuration.bean.system.Property"/>		
					<set-next-rule methodname="addProperty" paramtype="net.sf.minuteProject.configuration.bean.system.Property"/>	
					<set-properties-rule>
						<alias attr-name="name" prop-name="name" />
						<alias attr-name="value" prop-name="value" />
					</set-properties-rule>
				</pattern>										
				<pattern value="virtual-primary-key">
					<object-create-rule classname="net.sf.minuteProject.configuration.bean.enrichment.VirtualPrimaryKey"/>		
					<set-next-rule methodname="setVirtualPrimaryKey" paramtype="net.sf.minuteProject.configuration.bean.enrichment.VirtualPrimaryKey"/>		
					<set-properties-rule>
						<alias attr-name="name" prop-name="name" />	
						<alias attr-name="column-name" prop-name="columnName" />
					</set-properties-rule>			
					<pattern value="property">
						<object-create-rule classname="net.sf.minuteProject.configuration.bean.system.Property"/>		
						<set-next-rule methodname="addProperty" paramtype="net.sf.minuteProject.configuration.bean.system.Property"/>	
						<set-properties-rule>
							<alias attr-name="name" prop-name="name" />
							<alias attr-name="value" prop-name="value" />
							<alias attr-name="tag" prop-name="tag" />
						</set-properties-rule>
						<pattern value="property">
							<object-create-rule classname="net.sf.minuteProject.configuration.bean.system.Property"/>		
							<set-next-rule methodname="addProperty" paramtype="net.sf.minuteProject.configuration.bean.system.Property"/>	
							<set-properties-rule>
								<alias attr-name="name" prop-name="name" />
								<alias attr-name="value" prop-name="value" />
								<alias attr-name="tag" prop-name="tag" />
							</set-properties-rule>
						</pattern>	
					</pattern>						
				</pattern>
				<pattern value="field">
					<object-create-rule classname="net.sf.minuteProject.configuration.bean.enrichment.Field"/>		
					<set-next-rule methodname="addField" paramtype="net.sf.minuteProject.configuration.bean.enrichment.Field"/>		
					<set-properties-rule>
					    <alias attr-name="is-id" prop-name="isId" />
					    <alias attr-name="alias" prop-name="alias" />	
						<alias attr-name="name" prop-name="name" />	
						<alias attr-name="type" prop-name="type" />	
						<alias attr-name="length" prop-name="length" />	
						<alias attr-name="is-mandatory" prop-name="isMandatory" />	
						<alias attr-name="is-searchable" prop-name="searchable" />
						<alias attr-name="stereotype" prop-name="stereotype" />		
						<alias attr-name="comment" prop-name="comment" />
						<alias attr-name="description" prop-name="description" />																	
					</set-properties-rule>
					<pattern value="stereotype">
						<object-create-rule classname="net.sf.minuteProject.configuration.bean.enrichment.Stereotype"/>		
						<set-next-rule methodname="setStereotype" paramtype="net.sf.minuteProject.configuration.bean.enrichment.Stereotype"/>	
						<set-properties-rule>
							<alias attr-name="name" prop-name="name" />
							<alias attr-name="stereotype" prop-name="stereotype" />
						</set-properties-rule>
						<pattern value="formula">
							<object-create-rule classname="net.sf.minuteProject.configuration.bean.enrichment.Formula"/>		
							<set-next-rule methodname="setFormula" paramtype="net.sf.minuteProject.configuration.bean.enrichment.Formula"/>	
							<set-properties-rule>
								<alias attr-name="name" prop-name="name" />
								<alias attr-name="action" prop-name="action" />
								<alias attr-name="validation" prop-name="is-validation" />
							</set-properties-rule>
						</pattern>
					</pattern>
							
					<pattern value="property">
						<object-create-rule classname="net.sf.minuteProject.configuration.bean.system.Property"/>		
						<set-next-rule methodname="addProperty" paramtype="net.sf.minuteProject.configuration.bean.system.Property"/>	
						<set-properties-rule>
							<alias attr-name="name" prop-name="name" />
							<alias attr-name="tag" prop-name="tag" />
							<alias attr-name="value" prop-name="value" />
						</set-properties-rule>
						<pattern value="property">
							<object-create-rule classname="net.sf.minuteProject.configuration.bean.system.Property"/>		
							<set-next-rule methodname="addProperty" paramtype="net.sf.minuteProject.configuration.bean.system.Property"/>	
							<set-properties-rule>
								<alias attr-name="name" prop-name="name" />
								<alias attr-name="value" prop-name="value" />
								<alias attr-name="tag" prop-name="tag" />
							</set-properties-rule>
						</pattern>							
					</pattern>							
				</pattern>
			</pattern>		
					
