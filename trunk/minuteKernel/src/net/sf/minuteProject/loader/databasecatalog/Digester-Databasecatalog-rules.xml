
<!DOCTYPE digester-rules PUBLIC 
        "-//Jakarta Apache //DTD digester-rules XML V1.0//EN" 
        "http://jakarta.apache.org/commons/digester/dtds/digester-rules.dtd">
<digester-rules>
 <pattern value="database">
  <object-create-rule classname="net.sf.minuteProject.loader.databasecatalog.node.Database"/>
  <set-next-rule methodname="setDatabase" paramtype="net.sf.minuteProject.loader.databasecatalog.node.Database"/>
  <set-properties-rule/>
  <bean-property-setter-rule  propertyname="name" pattern="name"/>
  <bean-property-setter-rule  propertyname="version" pattern="version"/>
  <bean-property-setter-rule  propertyname="hibernateDialect" pattern="hibernate-dialect"/>
  <bean-property-setter-rule  propertyname="useSchema" pattern="use-schema"/>
  <bean-property-setter-rule  propertyname="driverclassname" pattern="driverClassName"/>
  <pattern value="primary-key-policy">
   <object-create-rule classname="net.sf.minuteProject.loader.databasecatalog.node.PrimaryKeyPolicy"/>
   <set-next-rule methodname="setPrimaryKeyPolicy" paramtype="net.sf.minuteProject.loader.databasecatalog.node.PrimaryKeyPolicy"/>
   <set-properties-rule/>
   <bean-property-setter-rule  propertyname="type" pattern="type"/>
   <bean-property-setter-rule  propertyname="suffix" pattern="suffix"/>
   <bean-property-setter-rule  propertyname="prefix" pattern="prefix"/>
  </pattern>
 </pattern>

</digester-rules>