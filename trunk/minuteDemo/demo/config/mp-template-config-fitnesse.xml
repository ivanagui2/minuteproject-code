<target name="BackendOnBsla-for-fitnesse">		
	<architecture-target name="fitnesse" description="fitness templates">
	</architecture-target>	

	<template-target 
	   name="fitness CRUD Wiki templates" 
	   rootdir="../.."
       templatedir = "template" 
	   libdir="lib/BslaLibrary.vm"
	   tool="velocity"
	   outputdir="../output/petshopFitness"
	   package-root="TestFitnesseCrud"
	   belongToPackage="true">
	
