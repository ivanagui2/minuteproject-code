           ${dependentDaoInterfaceVariableName}.insert$dependentDomainClassName ($dependentDomainVariableName);
		   setInjected (${dependentDomainClassName}.class, $dependentDomainVariableName);
		}
        return $dependentDomainVariableName;
    }

