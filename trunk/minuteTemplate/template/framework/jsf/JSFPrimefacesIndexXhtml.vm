#exposeJSFModelSpecific()
#exposePrimefacesConfig()
#set($title="$model.name jsf web")
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
   "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:p="$primefacesNamespace">
<h:head>
	<title><ui:insert name="title">Welcome to $model.name jsf primefaces web app</ui:insert></title>
	<link rel="stylesheet" type="text/css" href="css/style.css" />
</h:head>
<h:body>
    <!--<h3>pegazlight Menu</h3>-->  
    <p:panel id="welcome" toggleable="true" header="Welcome to $model.name">
	<h:panelGrid columns="1" columnClasses="column" cellpadding="5">  
		<h:outputText value="Welcome to $model.name"/>
		<h:outputLink id="menu" value="#{request.contextPath}${modelMenuJSFImport}">
		<h:outputText value="To administrate click here" />
		</h:outputLink>
	</h:panelGrid>
    </p:panel>
</h:body>
</html>	
