      <dvt:o1Axis/>
      <dvt:y1Axis/>
      <dvt:legendArea automaticPlacement="AP_NEVER"/>
      <dvt:o1MajorTick id="o1MajorTick1" tickStyle="GS_AUTOMATIC"/>
